{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": false,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "baseUrl": ".",
    "rootDirs": [".", "types/generated"],
    "paths": {
      "skeleton-app/*": [
        "app/*",
        "types/generated/app/*"
      ]
    }
  },
  "include": [
    "app",
    "types"
  ]
}
